{
  "env": {
    "commonjs": {
      "presets": [
        ["@babel/env", {
          "debug": true,
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 11"
            ]
          }
        }]
      ],
      "plugins": ["@babel/plugin-transform-runtime"]
    },
    "esm": {
      "presets": [
        ["@babel/env", {
          "modules": false
        }]
      ]
    },
    "umd": {
      "presets": [
        ["@babel/env", {
          "debug": true,
          "modules": "commonjs",
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 11"
            ]
          }
        }]
      ],
      "plugins": ["@babel/plugin-transform-runtime"]
    },
    "test": {
      "presets": ["@babel/env"]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ]
}

